   Matthews, William, UCL, Dept Anthropol, 14 Taviton St, London WC1H 0BW, England.},
DOI = {10.14318/hau7.1.020},
ISSN = {2575-1433},
EISSN = {2049-1115},
Keywords = {Descola; China; Yi Jing; cosmology; ontology; divination; analogy;
