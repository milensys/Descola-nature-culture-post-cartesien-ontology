@article{ WOS:000392783500011,
Author = {Grana-Behrens, Daniel},
Title = {With Descartes against Descartes On the Theory of Views on the Forms of
   Relations between Men and Non-men in Philippe Descola},
Journal = {ANTHROPOS},
Year = {2016},
Volume = {111},
Number = {2},
Pages = {533-551},
Abstract = {Philippe Descola's theory of different perceptions structuring the
   relations between humans and nonhumans and based on phylogenetic
   dispositions is moulded by the interplay of ontogenesis, collective
   practices, and prevailing value systems. He succeeds in processing the
   cartesian dichotomy between nature and culture in favor of alternative
   ontologies and abandoning the cultural view in favor of a uniform
   nature. His outcome is traced back to the most important
   anthropological, cognitive, and philosophical principles, and subsequent
   consequences and possibilities for anthropological studies are explored
   in response and anticipation of how humans and nonhumans including an
   organic entities may cohabit in the future.},
